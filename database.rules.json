{
  "rules": {
    ".read": true,
    ".write": true,
    "hives": {
      ".indexOn": ["name"],
      "$hive": {
        "projects": {
          ".indexOn": ["parentProject"]
        },
        "executions": {
          ".indexOn": ["project"]
        }
      }
    },
    "parentCache": {
      ".indexOn": ".value"
    }
  }
}
